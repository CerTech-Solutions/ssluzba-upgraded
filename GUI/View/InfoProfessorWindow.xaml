<Window x:Class="GUI.InfoProfessorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="Professor info"
        Height="475" Width="400"
        ResizeMode="NoResize">

    <Grid>
        <TextBox x:Name="textBoxSearch" HorizontalAlignment="Center" VerticalAlignment="Top" Height="20" Width="200" Margin="0,10,0,0" TextChanged="textBoxSearch_TextChanged"></TextBox>
        <ListView VerticalAlignment="Top" Margin="10,40,10,0" x:Name="listViewStudents" Height="380">
            <ListView.View>
                <GridView>
                    <GridViewColumn DisplayMemberBinding="{Binding NameSurname}">
                        <GridViewColumnHeader IsEnabled="False">Students</GridViewColumnHeader>
                    </GridViewColumn>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
